nav
{
    display: flex;
    padding: 2% 6%;
    justify-content: space-between;
    align-items: center;
    /* border: 1px solid white; */
}
nav img
{
    width: 150px;
    /* border: 1px solid white; */
}
.nav-links
{
    flex: 1;
    text-align: right;
    /* border: 1px solid white; */
}

.nav-links ul li
{
    list-style: none;
    display: inline-block;
    padding: 8px 12px;
    position: relative;
    text-transform: capitalize;
}
.nav-links ul li a
{
    text-decoration: none;
    color: white;
    width:13px ;
    font-family: 'Poppins', sans-serif;
}
.nav-links ul li::after
{
    content: '';
    width: 0%;
    height: 2px;
    background-color:rgb(176, 131, 131) ;
    margin: auto;
    display: block;
    transition: width 0.5s;
}
.nav-links ul li:hover::after
{
    width: 100%;
}
.text-box
{
    width: 80%;
    color: white;
    position: absolute;
    top: 50%;
    left: 50%;
    transform:translate(-50%,-50%);
    text-align: center;
}
.text-box h1 
{
    font-size: 62px;
    text-transform: capitalize;
}
.text-box p
{
    margin: 10px 0 40px;
    font-size:14px ;
    color: white;
}
.hero-btn
{
    display: inline-block;
    text-decoration: none;
    color: white;
    border: 1px solid white;
    background:transparent ;
    padding: 12px 34px;
    font-size:13px ;
    position: relative;
    cursor: pointer;
    transition: border 2s, background-color 2s;
}
.hero-btn:hover
{
    border: 1px solid #f44336;
    background-color:#f44336 ;
   
}

nav .fa
{
    display: none;
}
@media(max-width:700px)
{
    .text-box h1
    {
        font-size:30px ;
    }
    .nav-links ul li
    {
        display: block;
    }
    .nav-links
    {
        position: absolute;
        display: none;
        background-color: #a24c46;
        height: 100vh;
        top: 0;
        right: 0;
        text-align: left;
        z-index:2;
        width:200px;
        transition:1s;
    }
    .nav-links ul
    {
         padding: 20px;
    }
    nav .fa
    {
        display: block;
        color: white;
        margin: 10px;
        font-size: 22px;
        cursor: pointer;
    }
}
